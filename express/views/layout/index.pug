extends ./../layout.pug

block content
  each main in mains
    main.col-md-9.ml-sm-auto.pt-3.px-4
      h1= `${main.title}`
      hr
      div(id=`${main.id}`)
      script(src=`./dist/${main.id}.js`)
      each date in main.store
        script= `store.onCheckDate('${date.id}', '${date.date}')`
    