extends layout

block content
  .row
    nav.col-md-3.d-none.d-md-block.bg-light.sidebar
      .sidebar-sticky
        ul.nav.flex-column
          li.nav-item
            a.nav-link.active active
          li.nav-item
            a.nav-link link
          li.nav-item
            a.nav-link link
    main.col-md-9.ml-sm-auto.px-4.pt-3
      h1= tablename
      div.table-responsive
        table.table.table-striped
          thead
            tr
              th='id'
              th='username'
              th='root'
          tbody
            each date in tabledate
              tr
                th= date.id
                td= date.username
                td= date.root