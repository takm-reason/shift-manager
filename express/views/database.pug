extends layout

block content
  .row
    nav.col-md-3.d-none.d-md-block.bg-light.sidebar
      .sidebar-sticky
        ul.nav.flex-column
          li.nav-item
            a.nav-link.active active
          li.nav-item
            a.nav-link link
          li.nav-item
            a.nav-link link
    main.col-md-9.ml-sm-auto.px-4.pt-3
      h1= tablename
      div.table-responsive
        table.table.table-striped
          thead
            tr
              each val, index in tabledate[0]
                th= index
          tbody
            each date in tabledate
              tr
                each val, index in date
                  th= val
